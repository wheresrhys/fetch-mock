{
	"changelog-type": "default",
	"changelog-sections": [
		{
			"type": "feat",
			"section": "Features",
			"hidden": false
		},
		{
			"type": "fix",
			"section": "Bug Fixes",
			"hidden": false
		},
		{
			"type": "docs",
			"section": "Documentation Changes",
			"hidden": false
		},
		{
			"type": "chore",
			"section": "Miscellaneous",
			"hidden": true
		}
	],
	"pull-request-header": ":rock: I've created a release for you",
	"prerelease": true,
	"bump-minor-pre-major": true,
	"bump-patch-for-minor-pre-major": true,
	"plugins": [
		"node-workspace"
	],
	"packages": {
		"packages/fetch-mock": {}
	}
}
